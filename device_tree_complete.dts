/dts-v1/;

/ {
	compatible = "econet,en7523\0airoha,en7523";
	interrupt-parent = <0x01>;
	#address-cells = <0x01>;
	#size-cells = <0x01>;

	serial@1fbf0000 {
		reg-io-width = <0x04>;
		compatible = "ns8250";
		status = "okay";
		interrupts = <0x00 0x12 0x04>;
		reg = <0x1fbf0000 0x30>;
		clock-frequency = <0x1c2000>;
		reg-shift = <0x02>;
	};

	xsi@1fa04000 {
		compatible = "econet,ecnt-xsi";
		reg = <0x1fa08000 0x300 0x1fa04000 0x300 0x1fa05000 0x300 0x1fa07000 0x300 0x1fa09000 0x300>;
	};

	reserved-memory {
		ranges;
		#address-cells = <0x01>;
		#size-cells = <0x01>;

		atf-reserved-memory@80000000 {
			compatible = "econet,ecnt-atf-reserved-memory";
			no-map;
			reg = <0x80000000 0x40000>;
		};

		npu_binary@84000000 {
			no-map;
			phandle = <0x06>;
			reg = <0x84000000 0x100000>;
		};
	};

	serial@1fbf0300 {
		compatible = "econet,ecnt-uart2";
		interrupts = <0x00 0x20 0x04 0x00 0x36 0x04>;
		reg = <0x1fbf0300 0x30 0x1fbe1000 0x40>;
	};

	gdma@1fb30000 {
		compatible = "econet,ecnt-gdma";
		reg = <0x1fb30000 0x2b0>;
	};

	dma-controller@1fa01800 {
		#dma-cells = <0x01>;
		compatible = "econet,ecnt-hsdma";
		dma-requests = <0x02>;
		dma-channels = <0x02>;
		interrupts = <0x00 0x3f 0x04>;
		reg = <0x1fa01800 0x300>;
	};

	thermal_phy@1efbd000 {
		compatible = "airoha,en7552-thermal_phy";
		#thermal-sensor-cells = <0x00>;
		phandle = <0x0d>;
		reg = <0x1efbd000 0xfff>;
		int_name = "ptp_therm";
	};

	gpio@1fbf0200 {
		compatible = "airoha,airoha-gpio";
