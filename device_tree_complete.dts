		#gpio-cells = <0x02>;
		gpio_base = <0x00>;
	};

	mdio-bus {
		compatible = "airoha,arht-mdio";

		phy@15 {
			reg = <0x0f>;
		};

		phy@13 {
			reg = <0x0d>;
		};

		phy@14 {
			reg = <0x0e>;
		};
	};

	opp-table {
		compatible = "operating-points-v2";
		phandle = <0x04>;
