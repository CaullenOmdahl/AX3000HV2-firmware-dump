		interrupts = <0x00 0x2b 0x04>;
		reg = <0x1faf0000 0x1fff 0x1fa2ff24 0x04>;
	};

	chosen {
		stdout-path = "/serial@1fbf0000";
		bootargs = "sdram_conf=0x00108893 vendor_name=ECONET Technologies Corp. product_name=xPON ONU ethaddr=20:be:b4:20:b9:b6 snmp_sysobjid=1.2.3.4.5 country_code=ff ether_gpio=0c power_gpio=1b1b dsl_gpio=0b internet_gpio=02 multi_upgrade_gpio=0b020400000000000000000000000000 onu_type=2 qdma_init=33 root=/dev/mtdblock6 ro console=ttyS0,115200n8 earlycon bootflag=1 serdes_sel=0 serdes_sel=0 tclinux_info=0x15876df,0x4cd0,0x3443d0,0x349198,0x1240000,0x1855497,0x4d60,0x3520f8,0x356f50,0x1500000 ";
	};

	xpon@1fb64000 {
		compatible = "econet,ecnt-xpon";
		interrupts = <0x00 0x2a 0x04 0x00 0x22 0x04>;
		reg = <0x1fb64000 0x3e8 0x1fb66000 0x23c>;
	};

	xhci@1fab0000 {
		compatible = "econet,ecnt-xhci";
		interrupts = <0x00 0x21 0x04>;
		reg = <0x1fab0000 0x3e00 0x1fab3e00 0x100>;
	};

	thermal-zones {

		cpu-thermal {
			thermal-sensors = <0x0d 0x00>;
			polling-delay = <0x3e8>;
			polling-delay-passive = <0xc8>;

			trips {

				cpu-passive {
					hysteresis = <0x00>;
					temperature = <0x1e848>;
					type = "passive";
					phandle = <0x0e>;
				};
			};

			cooling-maps {

				map0 {
					trip = <0x0e>;
					cooling-device = <0x02 0xffffffff 0xffffffff 0x03 0xffffffff 0xffffffff>;
				};
			};
		};
	};

	pcm@bfbd0000 {
		compatible = "econet,ecnt-pcm";
		interrupts = <0x00 0x1b 0x04>;
		reg = <0x1fbd0000 0x4fff>;
	};

