		opp-8000000000 {
			opp-hz = <0x00 0x2faf0800>;
		};

		opp-550000000 {
			opp-hz = <0x00 0x20c85580>;
		};

		opp-600000000 {
			opp-hz = <0x00 0x23c34600>;
		};

		opp-7500000000 {
			opp-hz = <0x00 0x2cb41780>;
		};

		opp-9000000000 {
			opp-hz = <0x00 0x35a4e900>;
		};

		opp-650000000 {
			opp-hz = <0x00 0x26be3680>;
		};

		opp-8500000000 {
			opp-hz = <0x00 0x32a9f880>;
		};

		opp-7000000000 {
			opp-hz = <0x00 0x29b92700>;
		};

		opp-9500000000 {
			opp-hz = <0x00 0x389fd980>;
		};

		opp-500000000 {
			opp-hz = <0x00 0x1dcd6500>;
		};
	};

	wed_test {
		compatible = "en751221,wed_test";
		wed_num = <0x02>;
		reg = <0x1fa02b00 0x100 0x1fa03b00 0x100>;
	};

	ipsec@1fbfa000 {
		compatible = "econet,ecnt-ipsec";
		interrupts = <0x00 0x4f 0x04>;
		reg = <0x1fbfa000 0x268>;
	};

	sram@1fa40000 {
		compatible = "econet,ecnt-sram";
		reg = <0x1fa40000 0x8000 0x8000000 0x40000 0x1efc0000 0x40000 0x1e880000 0x40000 0x1fbe3000 0x200>;
	};

	apb_timer1@1fbf0100 {
		compatible = "econet,ecnt-timer";
		interrupts = <0x00 0x14 0x04 0x00 0x15 0x04 0x00 0x16 0x04 0x00 0x19 0x04>;
		reg = <0x1fbf0100 0x40>;
	};

	snor {
		compatible = "econet,ecnt-snor";
		spi-controller = <0x09>;
		spi2nfi = <0x0a>;
	};

	pbus_monitor@1fbe3400 {
		compatible = "econet,ecnt-pbus_monitor";
		reg = <0x1fbe3400 0xfc>;
	};

	spi_spi2nfi@1fa11000 {
		compatible = "econet,ecnt-spi2nfi";
		phandle = <0x0a>;
		reg = <0x1fa11000 0x160>;
	};

	spi_controller@1fa10000 {
		compatible = "econet,ecnt-spi_ctrl";
