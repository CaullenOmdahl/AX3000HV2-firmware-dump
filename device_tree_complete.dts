		phandle = <0x09>;
		reg = <0x1fa10000 0x140 0x00 0x1000>;
	};

	spi_ecc@1fa12000 {
		compatible = "econet,ecnt-spi_ecc";
		phandle = <0x0b>;
		reg = <0x1fa12000 0x150>;
	};

	xpon_xsgmii@1fa80000 {
		int_id = <0x00>;
		compatible = "airoha,air-xpon_xsgmii";
		reg = <0x1fa80a00 0x168 0x1fa80000 0x64 0x1fa86000 0x3ff>;
		int_name = "xpon_xsgmii";
	};

	airoha-trng@1faa1000 {
		compatible = "airoha,airoha-trng";
		interrupts = <0x00 0x23 0x04>;
		reg = <0x1faa1000 0xc04>;
	};

	i2s@1fbe2200 {
		compatible = "econet,ecnt-i2s";
		interrupts = <0x00 0x30 0x04>;
		reg = <0x1fbe2200 0xfc 0x1fbe2e00 0x114>;
	};

	wed2 {
		compatible = "en751221,wed2";
		wed_num = <0x02>;
		interrupts = <0x00 0x43 0x04 0x00 0x44 0x04>;
		reg = <0x1fa02000 0xb00 0x1fa03000 0xb00>;
		pci_slot_map = <0x00 0x01>;
	};

	leds {
		compatible = "gpio-leds";

		internet {
			gpios = <0x0c 0x14 0x01>;
			label = "internet";
			linux,default-trigger = "timer";
		};

		voip_hook {
			gpios = <0x0c 0x02 0x01>;
			label = "voip_hook";
			linux,default-trigger = "timer";
		};

		power {
			gpios = <0x0c 0x1b 0x01>;
			label = "power";
			linux,default-trigger = "timer";
		};

		eth3 {
			gpios = <0x0c 0x0a 0x01>;
			label = "eth3";
			linux,default-trigger = "none";
		};

		xponLos {
			gpios = <0x0c 0x03 0x01>;
			label = "xponLos";
			linux,default-trigger = "timer";
		};

		voip_reset {
			gpios = <0x0c 0x01 0x00>;
			label = "voip_reset";
			linux,default-trigger = "none";
		};

		eth1 {
			gpios = <0x0c 0x08 0x01>;
			label = "eth1";
			linux,default-trigger = "none";
		};

		mesh {
			gpios = <0x0c 0x15 0x01>;
			label = "mesh";
			linux,default-trigger = "timer";
		};

		xponStatus {
			gpios = <0x0c 0x07 0x01>;
			label = "xponStatus";
			linux,default-trigger = "timer";
		};

		eth4 {
			gpios = <0x0c 0x0b 0x01>;
			label = "eth4";
			linux,default-trigger = "none";
		};

		phy_tx_power_disable {
			gpios = <0x0c 0x0e 0x00>;
			label = "phy_tx_power_disable";
			default-state = "on";
			linux,default-trigger = "dsldev";
		};

		eth2 {
			gpios = <0x0c 0x09 0x01>;
			label = "eth2";
			linux,default-trigger = "none";
		};
	};

	i2c@1fbf8000 {
		compatible = "airoha,airoha-i2c";
		reg = <0x1fbf8000 0x65>;
		clock-frequency = <0x61a80>;
	};

	psci {
		compatible = "arm,psci-0.2";
		method = "smc";
	};

	usb_phy@1fac0000 {
		compatible = "econet,ecnt-usb_phy";
		reg = <0x1fac0000 0x1400>;
	};

	partitions {
		compatible = "fixed-partitions";
		#address-cells = <0x01>;
		#size-cells = <0x01>;

		fpt_data@5880000 {
			label = "fpt_data";
			reg = <0x5880000 0x500000>;
		};

		user_app@2880000 {
			label = "user_app";
			reg = <0x4580000 0xb00000>;
